{
  "name": "multi-environment",
  "version": "0.0.1",
  "author": "log2c",
  "homepage": "https://www.google.com.hk",
  "private": true,
  "scripts": {
    "start": "ionic-app-scripts serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "serve:uat": "env=uat node ./scripts/proxy-set.js && ionic-app-scripts serve --env=uat",
    "serve:prod": "env=prod node ./scripts/proxy-set.js && ionic-app-scripts serve --env=prod"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "^4.17.0",
    "@ionic-native/google-maps": "^4.15.1",
    "@ionic-native/splash-screen": "4.15.0",
    "@ionic-native/status-bar": "4.15.0",
    "@ionic/storage": "2.2.0",
    "cordova-android": "7.1.2",
    "cordova-ios": "4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-googlemaps": "^2.4.6",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "rxjs": "5.5.11",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.0",
    "glob": "^7.1.3",
    "minimist": "^1.2.0",
    "object-assign-deep": "^0.4.0",
    "path": "^0.12.7",
    "typescript": "2.6.2",
    "typescript-require": "^0.2.10"
  },
  "description": "An Ionic project",
  "config": {
    "ionic_webpack": "./scripts/webpack.config.js"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-googlemaps": {
        "API_KEY_FOR_ANDROID": "prod-google-maps-key-android",
        "API_KEY_FOR_IOS": "prod-google-maps-key-ios",
        "PLAY_SERVICES_VERSION": "15.0.1",
        "ANDROID_SUPPORT_V4_VERSION": "27.+",
        "LOCATION_WHEN_IN_USE_DESCRIPTION": "This app wants to get your location while this app runs only.",
        "LOCATION_ALWAYS_USAGE_DESCRIPTION": "This app wants to get your location always, even this app runs in background."
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
